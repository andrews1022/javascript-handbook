  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "babel-loader": "^8.0.5",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "json-server": "^0.15.1"
  }
}
